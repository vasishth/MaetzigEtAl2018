@article{Said2015,
abstract = {Most cognitive models contain adaptable parameters which determine model behavior. This flexibility, however, makes it difficult to intuitively understand the process behavior because of the wide range of possible parameter combinations. Thus, in order to investigate the dependence of the model's behavior from the parameters, systematic methods are necessary. However, methods like complete enumeration of the parameter space are only applicable to very small problems due to the high computational effort involved (curse of dimensionality). Therefore, we investigated applying optimization-based methods for parameter identification. As a proof of concept, we used a simple instance-based learning paradigm the Sugar Factory (Berry {\&} Broadbent, 1984) implemented in the ACT-R cognitive architecture. A time-discrete reformulation of our model allowed us to conduct a parameter sensitivity analysis using parallel computing which already provides a considerable speedup compared to a conventional parameter space search. In a second step we determined the parameter combinations which yield the best performance results for different inputs by applying and comparing several derivative-free algorithms. First steps towards a reformulation of the model in order to apply a derivative-based optimization approach are discussed.},
author = {Said, Nadia and Engelhart, Michael and Kirches, Christian and K{\"{o}}rkel, Stefan and Holt, Daniel V.},
file = {:Users/paul/ownCloud/potsdam{\_}msc{\_}ling/msc-thesis-maetzig/readings/Said et al 2015 Mathematical Formalization ACT-R Sugar Factory.pdf:pdf},
journal = {Proceedings of the 13th International Conference on Cognitive Modeling},
keywords = {ACT-R,Derivative- based optimization,Instance-based learning,Nonlinear programming,Sensitivity analysis},
number = {1984},
pages = {25--31},
title = {{Mathematical Formalization and Optimization of an ACT-R Instance-Based Learning Model}},
volume = {2},
year = {2015}
}
