issn = {0883-4237},
journal = {Statistical Science},
keywords = {Explanatory modeling, causality, predictive modeling, predictive power, statistical strategy, data mining, scientific research,and phrases,causality,data mining,eling,explanatory modeling,predictive mod-,predictive power,scientific research,statistical strategy},
number = {3},
